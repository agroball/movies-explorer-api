{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
      "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "func-names": ["error", "never"],
      "no-underscore-dangle": ["error", { "allow": ["_id"] }]
    }
}
